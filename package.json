{
  "name": "real-photo-booth",
  "version": "0.0.3",
  "description": "Real photo booth software",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ real-photo-booth-%npm_package_version% --platform win32 --arch x64 --out ./distribute/real-photo-booth-windows-%npm_package_version%/ --overwrite",
    "zip": "electron-installer-zip ./distribute/real-photo-booth-windows-%npm_package_version% ./distribute/installers/real-photo-booth-%npm_package_version%.zip"
  },
  "author": "Maxim Rubis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siburny/real-photo-booth.git"
  },
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-installer-zip": "^0.1.2",
    "electron-packager": "^12.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "gm": "^1.23.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2"
  },
  "optionalDependencies": {
    "node-webcam": "^0.4.5"
  }
}
